
egrep '<td> *[A-Z][a-z]+ +[A-Z][a-z]+ *</td> *<td> *([1-9][0-9]?|[12][0-9][0-9]|3[0-4][0-9]|350)/20(0[0-9]|1[0-7]) *</td>' nastava*.html -h \
| sed -r 's~.*<td>([ A-Za-z]+)</td> *<td> *([0-9]{1,3})/([0-9]{4}) *</td>.*~\1 \2 \3~' \
| sed -r 's/([0-9]{3}) [0-9]{2}([0-9]{2})/, mi\2\1/'\
| sed -r 's/([0-9]{2}) [0-9]{2}([0-9]{2})/, mi\20\1/'\
| sed -r 's/([0-9]) [0-9]{2}([0-9]{2})/, mi\200\1/'\
| sed -r 's/([A-Za-z]+) +([A-Za-z]+) +/\2 \1/'
